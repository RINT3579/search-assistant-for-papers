{
  "manifest_version": 3,
  "name": "Search Assistant For Papers",
  "version": "0.1.1",
  "description": "マウスでなぞった文字列を論文検索サイトで検索できます",
  "action": {

  },
  "content_scripts": [{
    "matches": ["<all_urls>"],
    "js": ["js/jquery-3.6.0.min.js","js/get-str-selection.js"],
    "run_at": "document_idle"
  }],
  "background": {
    "service_worker": "js/background.js",
    "type": "module"
  },
  "permissions": ["contextMenus", "tabs"]
}